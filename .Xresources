Xft.dpi: 96
Xft.autohint: 0
Xft.antialias: 1
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.rgba: rgb
Xft.lcdfilter: lcddefault

URxvt.font: xft:Inconsolata for Powerline:size=13
URxvt*saveLines: 10000
URxvt*scrollBar: false
URxvt.depth: 32
URxvt.background: [90]#282A36
